{
  "name": "arm-js-library",
  "version": "1.5.7",
  "description": "API Resource Manager",
  "type": "module",
  "files": ["dist", "types", "DOCS.md"],
  "main": "dist/arm-js-library.umd.js",
  "module": "dist/arm-js-library.es.js",
  "exports": {
    ".": {
      "import": "./dist/arm-js-library.es.js",
      "require": "./dist/arm-js-library.umd.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "build:jsdocs": "jsdoc2md src/lib/api-resource-manager.js > DOCS.md",
    "build:es-dts": "tsc dist/arm-js-library.es.js --declaration --allowJs --emitDeclarationOnly --outFile types/arm-js-library.es.d.ts --lib esnext --module esnext",
    "build:umd-dts": "tsc dist/arm-js-library.umd.js --declaration --allowJs --emitDeclarationOnly --outFile types/arm-js-library.umd.d.ts --lib esnext --module esnext",
    "build:dts": "npm run build:es-dts && npm run build:umd-dts"
  },
  "keywords": [
    "uuid",
    "state-management",
    "mobx",
    "json-api",
    "lodash",
    "crypto-js",
    "centralized-data",
    "javascript",
    "reactjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaeljymsgutierrez/arm-js-library.git"
  },
  "author": "Michael Jyms Gutierrez",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "eslint": "^9.5.0",
    "jsdoc-to-markdown": "^8.0.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.1",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "crypto-js": "^4.2.0",
    "lodash": "^4.17.21",
    "mobx": "^6.12.4",
    "uuid": "^10.0.0"
  }
}
