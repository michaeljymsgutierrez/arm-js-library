{
  "name": "arm-js-library",
  "version": "1.0.4",
  "description": "API Resource Manager",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/arm-js-library.umd.js",
  "module": "dist/arm-js-library.es.js",
  "exports": {
    ".": {
      "import": "./dist/arm-js-library.es.js",
      "require": "./dist/arm-js-library.umd.js"
    }
