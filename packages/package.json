  "keywords": [
    "arm",
    "api-resource-manager",
    "centralized-data-collection"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/michaeljymsgutierrez/arm-js-library.git"
  },
  "author": "Michael Jyms Gutierrez",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "eslint": "^9.5.0",
    "vite": "^5.3.1",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "crypto-js": "^4.2.0",
    "lodash": "^4.17.21",
    "mobx": "^6.12.4",
    "uuid": "^10.0.0"
  }
}
