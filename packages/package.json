{
  "name": "arm-js-library",
  "version": "2.0.1",
  "description": "API Resource Manager",
  "type": "module",
  "files": [
    "dist",
    "types",
    "DOCS.md"
  ],
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/arm-js-library.js",
      "types": "./types/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "build:jsdocs": "jsdoc2md --private src/lib/api-resource-manager.js > DOCS.md",
    "build:dts": "tsc dist/arm-js-library.js --declaration --allowJs --emitDeclarationOnly --outFile types/index.d.ts --lib esnext --module esnext --skipLibCheck",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "uuid",
    "state-management",
    "mobx",
    "json-api",
    "lodash",
    "md5",
    "centralized-data",
    "javascript",
    "reactjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaeljymsgutierrez/arm-js-library.git"
  },
  "author": "Michael Jyms Gutierrez",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.5.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "babel-jest": "^29.7.0",
    "eslint": "^9.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc-to-markdown": "^8.0.3",
    "miragejs": "^0.1.48",
    "typescript": "^5.5.4",
    "vite": "^5.4.10",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mobx": "^6.13.5",
    "uuid": "^11.0.4"
  }
}
